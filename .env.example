# Copy this to `.env` at your project root and fill in your key
HYPERCLOVA_API_KEY=YOUR_ACTUAL_HYPERCLOVA_API_KEY
# (optional)
DATABASE_URL=sqlite:///./test.db

# where Chroma will store its data
CHROMA_DB_DIR=.chroma_db

# your embedding model (dense) and sparse encoder (SPLADE)
EMBEDDING_MODEL_NAME=all-mpnet-base-v2
SPARSE_MODEL_NAME=naver/splade-v3
